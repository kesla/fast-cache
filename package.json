{
  "name": "fast-cache",
  "version": "1.1.0",
  "description": "Caches a method that returns a promise and does so really fast.",
  "main": "dist/index.js",
  "scripts": {
    "test": "xo && babel-tape-runner test.js",
    "build": "rm -rf dist && mkdir -p dist && babel lib --out-dir dist",
    "watch": "rm -rf dist && mkdir -p dist && babel -w lib --out-dir dist",
    "prepublish": "npm run build",
    "posttest": "readme package.json > readme.md"
  },
  "author": "David Bj√∂rklund <david.bjorklund@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "async-cache-promise": "^2.0.2",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-plugin-syntax-async-functions": "6.13.0",
    "babel-plugin-transform-async-to-generator": "6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "6.16.0",
    "babel-tape-runner": "^2.0.1",
    "package-json-to-readme": "^1.5.1",
    "tapava": "^2.2.0",
    "xo": "^0.16.0"
  },
  "xo": {
    "esnext": true,
    "space": true,
    "ignores": [
      "example.js"
    ]
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kesla/fast-cache.git"
  },
  "bugs": {
    "url": "https://github.com/kesla/fast-cache/issues"
  },
  "homepage": "https://github.com/kesla/fast-cache#readme"
}
